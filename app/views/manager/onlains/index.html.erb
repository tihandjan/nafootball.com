
<div class="well">
    <div class="nav nav-pills">
        <li role="presentation" class="active"><a class="btn btn-info" href="<%= new_manager_onlain_path %>">Создать Onlain</a></li>
    </div>
</div>
<div class="well">
    <div class="panel panel-primary">
        <div class="panel-heading">Список всех Onlain</div>
        <div class="panel-body">
            <div class="list-group">
                <% @onlains.each do |onlain| %>
                    <button type="button" class="list-group-item"><span style="color: #337ab7;"><%= onlain.id %></span> | <%= onlain.home_team %> | <%= onlain.date.strftime('%Y-%m-%d') %> | <%= onlain.away_team %> <%= link_to 'Удалить', manager_onlain_path(onlain), method: :delete, data: { confirm: 'Вы уверены?' }, class: 'btn btn-danger', style: 'float: right; margin-left: 10px;' %><a href="<%= edit_manager_onlain_path(onlain) %>" style="float: right;" class="btn btn-warning">Редактировать</a><%= link_to 'Просмотреть', match_path((onlain.home_team.split(' ').join('-')), (onlain.date.strftime('%Y-%m-%d')), (onlain.away_team.split(' ').join('-'))) , class: 'btn btn-success', style: 'float: right; margin-right: 10px;' %></button>
                <% end %>
            </div>
        </div>
    </div>
    <%= will_paginate @onlains %>
</div>